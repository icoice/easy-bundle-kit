{
  "name": "easy-bundle-kit",
  "version": "0.0.4",
  "description": "easy webpack develop",
  "main": "index.js",
  "scripts": {
    "debug-cmd": "set DEBUG=* & node index.js --webpack --entry ./examples --language typescript",
    "debug-shell": "DEBUG='*' & node index.js --webpack --entry ./examples --language typescript",
    "debug-powershell": "@powershell -Command $env:DEBUG='*'; node index.js --webpack --entry ./examples --language typescript",
    "test:all": "@powershell -Command $env:DEBUG='*'; node index.js --webpack -e ./examples -l typescript ecmascript pug",
    "test:ts": "@powershell -Command $env:DEBUG='*'; node index.js --webpack --entry ./examples --language typescript",
    "test:pug": "@powershell -Command $env:DEBUG='*'; node index.js -w -e ./examples -l pug",
    "test:es": "@powershell -Command $env:DEBUG='*'; node index.js -w -e ./examples -l ecmascript"
  },
  "keywords": [
    "webpack"
  ],
  "author": "icoice",
  "license": "MIT",
  "devDependencies": {
    "cli": "^1.0.1"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icoice/easy-bundle-kit.git"
  },
  "bugs": {
    "url": "https://github.com/icoice/easy-bundle-kit/issues"
  },
  "homepage": "https://github.com/icoice/easy-bundle-kit#readme"
}
